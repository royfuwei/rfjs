{
  "name": "@rfjs/orm-typeorm",
  "version": "0.0.0",
  "description": "A project created by lib-tsdown",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "private": true,
  "scripts": {
    "clean": "npx npm-run-all --parallel clean:dist clean:types",
    "clean:types": "npx rimraf ./types",
    "clean:dist": "npx rimraf ./dist",
    "dev": "npx npm-run-all --parallel dev:tsdown typecheck:watch",
    "dev:tsdown": "npm run clean && tsdown --config-loader unrun --watch",
    "build": "npm run build:tsdown",
    "build:tsdown": "npm run clean && tsdown --config-loader unrun",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "migration:run": "tsx src/run-migration.ts",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d src/ormconfig.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/ormconfig.ts",
    "migration:create": "typeorm-ts-node-commonjs migration:create",
    "schema:drop": "typeorm-ts-node-commonjs schema:drop -d src/ormconfig.ts",
    "lint-staged": "npx lint-staged",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "npm run vitest:run",
    "vitest": "vitest --passWithNoTests",
    "vitest:run": "vitest --passWithNoTests --run",
    "vitest:ui": "vitest --passWithNoTests --ui",
    "vitest:e2e": "vitest --config vitest.config.e2e.mts --passWithNoTests",
    "vitest:e2e:ui": "vitest --config vitest.config.e2e.mts --passWithNoTests --ui",
    "release": "npx standard-version",
    "commit": "npx cz",
    "preinstall": "npx -y only-allow pnpm",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "lint-staged": {
    "*.ts": [
      "npx vitest related --run",
      "npx prettier --write",
      "npx eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "keywords": [],
  "author": "royfuwei",
  "license": "ISC",
  "engines": {
    "node": ">=18",
    "pnpm": ">=10.24.0 <11.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@commitlint/cz-commitlint": "^19.6.1",
    "@eslint/js": "^9.20.0",
    "@types/pg": "^8.16.0",
    "@types/pg-connection-string": "^2.0.0",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-istanbul": "^3.2.3",
    "@vitest/ui": "^3.2.3",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "husky": "^9.1.7",
    "inquirer": "9",
    "lint-staged": "^15.4.3",
    "pg-connection-string": "^2.9.1",
    "prettier": "^3.5.1",
    "rimraf": "^6.0.1",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "tsdown": "0.17.0-beta.6",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.0",
    "vitepress": "^1.6.3",
    "vitest": "^3.2.3"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "tslib": "^2.8.1",
    "typeorm": "^0.3.28"
  }
}